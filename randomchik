package randomchik;


public class Solution {

    public static void main(String[] args) {
        int min = 0;
        int max = 101; // Потому что вероятность что  Math.random выдаст 1, крайне мала )))
        int random_number = min + (int) (Math.random()* max); // Генерация числа
        System.out.println("Случайное число, которое я загадал: " + random_number);

    }

}
